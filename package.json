{
  "name": "next-js-blog-template",
  "version": "0.0.1",
  "description": "Blog template with Next.js.",
  "engines": {
    "node": ">=12.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yopinoji/next-js-blog-template.git"
  },
  "homepage": "https://next-playground.vercel.app",
  "private": true,
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc"
  },
  "dependencies": {
    "autoprefixer": "10.3.1",
    "esbuild": "0.12.20",
    "github-slugger": "1.3.0",
    "gray-matter": "4.0.3",
    "mdx-bundler": "6.0.1",
    "next": "11.1.2",
    "next-themes": "0.0.14",
    "postcss": "8.3.6",
    "preact": "10.5.14",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "reading-time": "1.3.0",
    "rehype-autolink-headings": "6.0.0",
    "rehype-prism-plus": "1.0.0",
    "rehype-slug": "5.0.0",
    "remark-footnotes": "4.0.1",
    "remark-gfm": "2.0.0",
    "remark-math": "5.1.0",
    "rss": "1.2.2",
    "tailwindcss": "2.2.7"
  },
  "devDependencies": {
    "@types/react": "17.0.21",
    "@types/react-dom": "17.0.9",
    "@types/rss": "0.0.29",
