{
  "name": "next-js-blog-template",
  "version": "0.0.2",
  "description": "Blog template with Next.js.",
  "repository": {
    "type": "git",
    "url": "https://github.com/qlawmarq/next-markdown-blog-YoRHa.git"
  },
  "license": "MIT",
  "author": "Masaki Yoshiiwa <masaki.yoshiiwa@gmail.com> (http://qlawmarq.net/)",
  "homepage": "https://next-playground.vercel.app",
  "engines": {
    "node": "18.x"
  },
  "private": true,
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write --ignore-path .gitignore './**/*.{js,jsx,ts,tsx,json,css}'",
    "typecheck": "tsc",
    "prepare": "husky install",
    "test:e2e": "playwright test",
    "update:e2e": "playwright test -u"
  },
  "dependencies": {
    "@emotion/babel-preset-css-prop": "^11.11.0",
    "@emotion/react": "^11.11.1",
    "github-slugger": "^2.0.0",
    "next": "^14.1.1",
    "next-mdx-remote": "4.1.0",
    "next-seo": "^6.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rehype-prism-plus": "2.0.0",
    "rehype-slug": "^6.0.0",
    "rehype-toc": "3.0.2",
    "remark-footnotes": "4.0.1",
    "remark-gfm": "3.0.1",
    "remark-math": "6.0.0",
    "rss": "1.2.2"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.0.2",
    "@playwright/test": "^1.39.0",
    "@types/github-slugger": "1.3.0",
    "@types/node": "^18.19.4",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "@types/rss": "0.0.32",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "eslint-config-next": "^14.0.2",
    "eslint-config-prettier": "^9.0.0",
    "file-loader": "6.2.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.1.0",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
  }
}
